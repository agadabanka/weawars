package  {		import flash.display.*;	import flash.events.MouseEvent;	import flash.geom.Point;	import Field;	public class Range extends Shape{		private static const WHITE:int = 0xFFFFFF;			public function Range( grid_size:Number ){			graphics.beginFill( WHITE ); //a white circle indicates range			graphics.drawCircle( (grid_size/2), (grid_size/2), 3*grid_size );			graphics.endFill( );			alpha = 0.3; //for debugging		}	}}